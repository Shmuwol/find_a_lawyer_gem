  Avvo.withContext(function(context){
    var embeddedSearch = $('*[data-search-query]').data('search-query');

    if (embeddedSearch) {
      $('.js-search-textfield').val(embeddedSearch);
    } else {
      $('.js-search-textfield').val(context.keyword);
    }

    var embeddedLocation = $('*[data-location-query]').data('location-query');

    if (context.location) {
      $('.js-location-textfield').val(context.location.short_name);
    } else if (embeddedLocation) {
      $('.js-location-textfield').val(embeddedLocation);
    }
  })
});</script><div id="skip_to_content"></div><div content="416"></div><link rel="stylesheet" media="screen" href="/assets/amos_responsive/views/lawyers/search-a2dee905efc58e6bab4a8fc304fe4f0df77d14c32cab4b0aa1ee2571f496d5dc.css" /><div class="container"><div class="u-vertical-padding-3-halves"><h1 class="strong">Speeding Ticket lawyers in zip code: 11225 (Brooklyn, NY) <small> (416)</small></h1><p class="text-muted">Compare Speeding Ticket attorneys near you. Read client reviews, see Avvo Ratings, and contact them directly.</p></div><div class="row"><div class="col-xs-12 col-sm-4 pull-right"><div id="filters-collapse" class="u-margin-bottom-1"><div id="filters" class="collapse"><div class="card accent-border-top"><form><div class="form-group form-control-with-icons"><label class="control-label" for="sort">Sort by</label><select id="sort" class="form-control" data-search-filter="sort" data-search-filter-type="single-value"><option selected="selected" value="relevancy">Best Match</option>
