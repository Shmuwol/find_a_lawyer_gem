  Avvo.withContext(function(context){
    var embeddedSearch = $('*[data-search-query]').data('search-query');

    if (embeddedSearch) {
      $('.js-search-textfield').val(embeddedSearch);
    } else {
      $('.js-search-textfield').val(context.keyword);
    }

    var embeddedLocation = $('*[data-location-query]').data('location-query');

    if (context.location) {
      $('.js-location-textfield').val(context.location.short_name);
    } else if (embeddedLocation) {
      $('.js-location-textfield').val(embeddedLocation);
    }
  })
