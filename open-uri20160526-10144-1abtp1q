  $('a.nav-section-action').click(function(event) {
    var dropdownIconClicked    = $(event.target).hasClass('icon'),
        hasContent             = $(this).next().hasClass('nav-section-content'),
        contentVisible         = $(this).next().is(':visible');

    function goToUrl() {
      if(hasContent && contentVisible) {
        event.stopPropagation();
      }
    };

    function toggleDropdown(){
      if(dropdownIconClicked || ( hasContent && !contentVisible )) {
        event.preventDefault();
      }
      else {
        goToUrl();
      }
    };

    toggleDropdown();
  });
});</script></nav><div class="header-group header-visible-min header-links"><button class="header-link" id="search-toggle" type="button" data-toggle="collapse" data-target="#header-search-collapse" aria-expanded="false" aria-controls="#header-search-collapse" data-parent="#header"><span aria-hidden="true" class="icon icon-search"></span></button></div></div><div class="header-visible-min"><div class="collapse header-collapse" id="header-search-collapse" role="search"><div class="u-bg-lighter-gray"><div class="container"><div class="row"><div class="col-xs-12"><form class="simple_form u-vertical-padding-gutter js-search-form js-search-twobox-section" action="/search/lawyer_search" accept-charset="UTF-8" method="get"><input name="utf8" type="hidden" value="&#x2713;" /><div class="form-group select optional global_search_topic form-group"><label class="select optional sr-only" for="global_search_topic">Search topic</label><div class="form-control-with-icons"><select class="select optional js-search-select form-control" name="global[search_topic]" id="global_search_topic"><option selected="selected" value="directory">Lawyers</option>
