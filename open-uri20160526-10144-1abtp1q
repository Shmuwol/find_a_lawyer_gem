  var $search = $('.js-search-select');
  var $textField = $('.js-search-textfield.js-mobile');
  var $form = $('.js-search-form');

  var searchConfig = {
    directory: {
      action: '/search/lawyer_search',
      placeholder: '"Business" – or – "John Smith"'
    },
    knowledgebase: {
      action: '/search/redirect',
      placeholder: 'Legal topic'
    }
  };

  $search.on('change', function () {
    var vertical = $search.val();
    var settings = searchConfig[vertical];

    if (settings) {
      $textField.attr('placeholder', settings.placeholder);
      $form.attr('action', settings.action);
    }
  });

